{
  "name": "root",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:pattern": "bun run --filter 'pattern-hero' build",
    "dev:pattern": "bun run --filter 'pattern-hero' dev",
    "install:pattern": "bun install --filter 'pattern-hero'",
    "build:qrcode": "bun run --filter 'beautiful-qrcode' build",
    "dev:qrcode": "bun run --filter 'beautiful-qrcode' dev",
    "install:qrcode": "bun install --filter 'beautiful-qrcode'",
    "build:waves": "bun run --filter 'waves-generator' build",
    "dev:waves": "bun run --filter 'waves-generator' dev",
    "install:waves": "bun install --filter 'waves-generator'",
    "format": "bun run --filter '*' format",
    "lint": "bun run --filter '*' lint"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "bun@1.3.9",
  "repository": {
    "type": "git",
