    "@penpot/plugin-styles": "1.4.2",
    "@penpot/plugin-types": "1.4.2",
    "@types/alpinejs": "^3.13.11",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.8",
    "alpinejs": "^3.14.9",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "qr-code-styling": "^1.9.2",
    "solid-js": "^1.9.11",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-live-preview": "^0.4.0",
    "vite-plugin-handlebars": "^2.0.0"
